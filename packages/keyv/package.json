{
  "name": "keyv",
  "version": "3.1.0",
  "description": "Simple key-value storage with support for multiple backends",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts --format pretty ./",
    "test": "nyc ava test/keyv.js",
    "test:full": "nyc ava --serial",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "xo-lukechilds",
      "xo-typescript",
      "plugin:@typescript-eslint/recommended-requiring-type-checking"
    ],
    "parserOptions": {
      "project": "tsconfig.json"
    },
    "rules": {
      "@typescript-eslint/no-unnecessary-condition": [
        "warn",
        {
          "ignoreRhs": true
        }
      ],
      "@typescript-eslint/strict-boolean-expressions": "off",
      "no-dupe-class-members": 0
    },
    "overrides": [
      {
        "files": [
          "**/*.js"
        ],
        "rules": {
          "@typescript-eslint/unbound-method": "off",
          "@typescript-eslint/explicit-function-return-type": "off"
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukechilds/keyv.git"
  },
  "keywords": [
    "key",
    "value",
    "store",
    "cache",
    "ttl"
  ],
  "author": "Luke Childs <lukechilds123@gmail.com> (http://lukechilds.co.uk)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lukechilds/keyv/issues"
  },
  "homepage": "https://github.com/lukechilds/keyv",
  "dependencies": {
    "json-buffer": "3.0.1",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@keyv/mongo": "*",
    "@keyv/mysql": "*",
    "@keyv/postgres": "*",
    "@keyv/redis": "*",
    "@keyv/sqlite": "*",
    "@keyv/test-suite": "*",
    "@types/json-buffer": "^3.0.0",
    "@types/node": "^12.7.11",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "ava": "^2.2.0",
    "coveralls": "^3.0.0",
    "eslint": "^6.5.1",
    "eslint-config-xo": "^0.27.1",
    "eslint-config-xo-lukechilds": "^1.0.0",
    "eslint-config-xo-typescript": "^0.19.0",
    "eslint-formatter-pretty": "^2.1.1",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.13",
    "this": "^1.0.2",
    "timekeeper": "^2.0.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  }
}
